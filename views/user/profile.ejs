        <!-- my account section start -->
        <section class="my__account--section section--padding">
            <div class="container">
                <p class="account__welcome--text">Hello,  welcome to your dashboard!</p>
                <div class="my__account--section__inner border-radius-10 d-flex">
                    <div class="account__left--sidebar">
                        <h2 class="account__content--title h3 mb-20">My Profile</h2>
                        <ul class="account__menu">
                            <li class="account__menu--list active"><a href="">Dashboard</a></li>
                            <li class="account__menu--list"><a href="/profiletwo">profile</a></li>
                            <li class="account__menu--list"><a href="/address">Addresses</a></li>
                            <li class="account__menu--list"><a href="/wishlist">Wishlist</a></li>
                            <li class="account__menu--list"><a href="/wallet">Wallet</a></li>
                            <li class="account__menu--list"><a href="/logout">Log Out</a></li>
                        </ul>
                    </div>
                    <div class="account__wrapper">
                        <div class="account__content">
                            <h2 class="account__content--title h3 mb-20">Orders History</h2>
                            <div class="account__table--area">
                                <table class="account__table">
                                    <thead class="account__table--header">
                                        <tr class="account__table--header__child">
                                            <th class="account__table--header__child--items">Order</th>
                                            <th class="account__table--header__child--items">Date</th>
                                            <th class="account__table--header__child--items">Payment Status</th>
                                            <th class="account__table--header__child--items">Oder Status</th>
                                            <th class="account__table--header__child--items">Total</th>
                                        </tr>
                                    </thead>
                                    <tbody class="account__table--body mobile__none">
                                        <% if (orders.length > 0) { %>
                                            <% orders.forEach(order => { %>
                                                <tr class="account__table--body__child">
                                                    <td class="account__table--body__child--items">
                                                        <% order.items.forEach(item => { %>
                                                            <img src="<%= item.image %>" alt="<%= item.productName %>" width="50" height="50" /> <%= item.productName %><br>
                                                        <% }); %>
                                                    </td>
                                                    <td class="account__table--body__child--items">
                                                        <% if (order.createdAt) { %>
                                                            <%= order.createdAt.toLocaleDateString() %>
                                                        <% } else { %>
                                                            <span>N/A</span>
                                                        <% } %>
                                                    </td>
                                                    <td class="account__table--body__child--items"><%= order.paymentStatus %></td>
                                                    <td class="account__table--body__child--items"> <%= order.status %></td>
                                                    <td class="account__table--body__child--items">$<%= order.totalAmount.toFixed(2) %></td>
                                                    <td class="account__table--body__child--items">
                                                        <a href="/order-details/<%= order._id %>" class="btn btn-primary">View Details</a>
                                                    </td>
                                                </tr>
                                            <% }); %>
                                        <% } else { %>
                                            <tr>
                                                <td colspan="6" class="text-center">No orders found.</td>
                                            </tr>
                                        <% } %>
                                        
                                        
                                    </tbody>
                                </table>
                            </div>
                            
                            <div class="pagination">
                                <!-- Previous Button -->
                                <% if (currentPage > 1) { %>
                                  <a href="/profile?page=<%= currentPage - 1 %>" class="pagination-link">Previous</a>
                                <% } %>
                              
                                <!-- Page Number Links (Display 5 pages at a time) -->
                                <% 
                                  const startPage = Math.max(1, currentPage - 2); // Start 2 pages before the current one
                                  const endPage = Math.min(totalPages, currentPage + 2); // End 2 pages after the current one
                                %>
                              
                                <% for (let i = startPage; i <= endPage; i++) { %>
                                  <a href="/profile?page=<%= i %>" class="pagination-link <%= currentPage === i ? 'active' : '' %>">
                                    <%= i %>
                                  </a>
                                <% } %>
                              
                                <!-- Next Button -->
                                <% if (currentPage < totalPages) { %>
                                  <a href="/profile?page=<%= currentPage + 1 %>" class="pagination-link">Next</a>
                                <% } %>
                              </div>
                              
                              
                              
                              
                        </div>
                    </div>
                    
                </div>
            </div>
        </section>
        <!-- my account section end -->

